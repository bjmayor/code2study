#include <stdio.h>
#define TABSPACE 8
main()
{
    char c;
    int position=0;
    int fillspace = 0;
    while ((c=getchar())!=EOF)
    {
        if (c=='\t')
        {
            fillspace = TABSPACE - position%TABSPACE;
            while(fillspace--)
            {
                putchar('');
            }
        }
        else if (c=='\n')
        {
            position = 0;
                }
        else
        {
            putchar(c);
            position++;
        }
    }
}